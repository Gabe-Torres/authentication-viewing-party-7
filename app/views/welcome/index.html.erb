<% if current_user %>
<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= link_to user.email, dashboard_path(user.id) %></li>
    <% end %>
</section>
  <%= link_to "Log Out", logout_path %>
<% else %>
  <%= button_to "Create New User", register_path, method: :get %>
  <%= link_to "I already have an account", login_path %>
<% end %>

